        "guzzlehttp/guzzle": "^6.3",
        "illuminate/contracts": "^5.5",
        "illuminate/container": "^5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "authors": [
        {
            "name": "Yandaxu",
